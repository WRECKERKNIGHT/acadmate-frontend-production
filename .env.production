# Production Environment Variables for ACADMATE Frontend
# This will be dynamically set by Netlify environment variables
VITE_API_URL=https://acadmate-backend-2025.onrender.com/api
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=production

# Performance & Monitoring
VITE_ENABLE_ERROR_BOUNDARY=true
VITE_ENABLE_LOADING_STATES=true

# Analytics (if needed)
# VITE_GA_TRACKING_ID=
# VITE_HOTJAR_ID=

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=false
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_OFFLINE_MODE=true
